{
    "Lua.runtime.version": "LuaJIT",
    "Lua.workspace.library": [
        "${3rd}/love2d/library"
    ],
    "Lua.workspace.checkThirdParty": false,
    "files.exclude": {
        "**/*.lua": {
            "when": "$(basename).ts"
        },
        "**/*?.lua": {
            "when": "$(basename).json"
        },
        "**/*.lua.map": {
            "when": "$(basename)"
        },
        "lualib_bundle.lua": true
    }
}
